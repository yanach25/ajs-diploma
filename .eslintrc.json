{
    "extends": "airbnb-base",
    "env": {
       "es6": true,
       "browser": true,
       "jest": true,
       "commonjs": true,
      "node": true
    },
    "rules": {
      "no-mixed-operators": "off",
      "no-plusplus": "off",
      "no-alert": "off",
      "no-shadow": "off",
      "max-len": ["error", { "code": 160 }],
      "no-restricted-syntax": [
        "error",
        "LabeledStatement",
        "WithStatement"
      ]
    }
  }
